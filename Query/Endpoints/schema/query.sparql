BASE <https://id.example.org/>
PREFIX : <https://id.example.org/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

CONSTRUCT {
    ?vocabularyResource ?vocabularyResourceProperty ?vocabularyResourceValue .
}
WHERE {
    ?vocabularyResource
        ?vocabularyResourceProperty ?vocabularyResourceValue ;
        rdfs:isDefinedBy :schema .
}
