PREFIX : <https://id.volunteeringdata.io/schema/>
PREFIX text: <http://jena.apache.org/text#>

SELECT ?s ?label ?value
WHERE {
  (?s ?score ?value) text:query (:organisationDescription :organisationLabel :organisationPurpose $query "highlight:s:<mark>|e:</mark>") .
  
  ?s :organisationLabel ?label .
}
ORDER BY DESC(?score)
