    rdfs:range volunteering:Location ;
    rdfs:label "location"@en .

volunteering:contact
    a rdf:Property ;
    rdfs:isDefinedBy :schema ;
    rdfs:range volunteering:Contact ;
    rdfs:label "contact"@en .


## Generic Properties
volunteering:label
    a rdf:Property ;
    rdfs:isDefinedBy :schema ;
    rdfs:subPropertyOf rdfs:label ;
    rdfs:label "label"@en .

volunteering:description
    a rdf:Property ;
    rdfs:isDefinedBy :schema ;
    rdfs:subPropertyOf dc:description ;
    rdfs:label "description"@en .

volunteering:url
    a rdf:Property ;
    rdfs:isDefinedBy :schema ;
    rdfs:subPropertyOf rdfs:seeAlso ;
    rdfs:label "url"@en .


